/* Pixel theme for start screen */

/* Tile the background to keep pixel crispness */
#page_startScreen {
    border-image: url(:/startScreen/background.png) 0 0 0 0 stretch stretch;
}

/* Tooltip styling (pixel theme) */
QToolTip {
    background-color: rgba(0, 0, 0, 0.85);
    color: #ffffff;
    border: 2px solid #ffffff;
    border-radius: 0;          /* crisp pixel edges */
    padding: 4px 6px;
    font-family: Fixedsys, &quot;Lucida Console&quot;, &quot;Courier New&quot;, monospace;
    font-size: 10pt;
}

QMessageBox {
    background-color: #3e3e3e;
    color: #f0f0f0;
    border: 2px solid #dcdcdc;
    border-radius: 0;
    padding: 8px 10px;
    font-family: Fixedsys, &quot;Lucida Console&quot;, &quot;Courier New&quot;, monospace;
    font-size: 11pt;
}

/* Primary text */
QMessageBox QLabel#qt_msgbox_label {
    color: #f0f0f0;
    background: transparent;
    padding: 0;
    margin: 6px 0 0 0;
    qproperty-alignment: AlignCenter;
}

QMessageBox QLabel#qt_msgboxex_icon_label {
    min-width: 0px;
    max-width: 0px;
    width: 0px;
    margin: 0;
    padding: 0;
}

/* Detailed text */
QMessageBox QLabel#qt_msgboxex_detailedtext_label {
    color: #dcdcdc;
    background: transparent;
    margin-top: 4px;
}

/* Center the buttons and add spacing */
QMessageBox QDialogButtonBox {
    qproperty-centerButtons: true;
    padding-top: 10px;
}
QMessageBox QPushButton {
    background-color: #5a5a5a;
    border: 2px solid #1e1e1e;
    border-radius: 0;
    padding: 6px 12px;
    color: #ffffff;
    font-family: Fixedsys, &quot;Lucida Console&quot;, &quot;Courier New&quot;, monospace;
    font-size: 10pt;
}
QMessageBox QPushButton:hover {
    background-color: #6a6a6a;
}
QMessageBox QPushButton:pressed {
    background-color: #4a4a4a;
    border-left-color: #2a2a2a;
    border-top-color: #2a2a2a;
    border-right-color: #777777;
    border-bottom-color: #777777;
}

/* Specific QMessageBox buttons by id */
QMessageBox QPushButton#ab_yes {
    background-color: #2b6f31;  
    border: 2px solid #1d4f22;  
    color: #ffffff;
}

QMessageBox QPushButton#ab_yes:hover { background-color: #357a3a; }
QMessageBox QPushButton#ab_yes:pressed {
    background-color: #235f26;  
    border-left-color: #133516;  
    border-top-color: #133516;   
    border-right-color: #5b8f60; 
    border-bottom-color: #5b8f60; 
}

QMessageBox QPushButton#ab_exit {
    background-color: #8f2d2d;  
    border: 2px solid #5e1e1e;  
    color: #ffffff;
}
QMessageBox QPushButton#ab_exit:hover { background-color: #9b3a3a; }
QMessageBox QPushButton#ab_exit:pressed {
    background-color: #7a2424;  
    border-left-color: #2e1515;  
    border-top-color: #2e1515;   
    border-right-color: #b76f6f; 
    border-bottom-color: #b76f6f; 
}

QMessageBox QPushButton#ab_startScreen {
    background-color: #1c4a80;  
    border: 2px solid #0c325f;  
    color: #ffffff;
}
QMessageBox QPushButton#ab_startScreen:hover { background-color: #245c9a; }
QMessageBox QPushButton#ab_startScreen:pressed {
    background-color: #163c68;  
    border-left-color: #0a253f; 
    border-top-color: #0a253f;  
    border-right-color: #5a89b7; 
    border-bottom-color: #5a89b7; 
}

/* QDialog pixel theme (lighter gray) */
QDialog {
    background-color: #4a4a4a;   /* darker gray */
    color: #f0f0f0;
    border: 2px solid #1e1e1e;
    border-radius: 0;
    padding: 8px 10px;
    font-family: Fixedsys, &quot;Lucida Console&quot;, &quot;Courier New&quot;, monospace;
    font-size: 10pt;
}
QDialog QLabel {
    color: #f0f0f0;
    background: transparent;
    font-family: Fixedsys, &quot;Lucida Console&quot;, &quot;Courier New&quot;, monospace;
}

/* Horizontal slider — thin, fixed size, full range */

QDialog QSlider:horizontal {
    min-height: 12px;
    max-height: 12px;       
}

QDialog QSlider::groove:horizontal {
    height: 2.5px;             
    background: #5a5a5a;
    border: 1px solid #1e1e1e;
}

QDialog QSlider::sub-page:horizontal {
    border: 1px solid #163816;
    background: qlineargradient(
        x1:0, y1:0, x2:1, y2:0,
        stop:0 rgb(40,200,40),
        stop:1 rgb(80,200,80)
    );
}

QDialog QSlider::add-page:horizontal {
    background: #5a5a5a;
    border: 1px solid #1e1e1e;
}

QDialog QSlider::handle:horizontal {
    width: 6px;
    height: 10px;
    background: #3a3a3a;
    border: 1px solid #000000;
    margin: -4px -1px -4px -1px; /* center handle on groove */  
}


#page_startScreen QLabel,
#page_startScreen QPushButton,
#page_startScreen QGroupBox,
#page_startScreen QComboBox,
#page_startScreen QKeySequenceEdit,
#page_startScreen QSpinBox,
#page_startScreen QDoubleSpinBox,
#page_startScreen QAbstractSpinBox {
    font-family: Fixedsys, &quot;Lucida Console&quot;, &quot;Courier New&quot;, monospace;
    font-size: 10pt;
    color: #ffffff;
}

/* GroupBox with pixel border and title */
#page_startScreen QGroupBox {
    border: 2px solid #000000;
    border-radius: 0;
    margin-top: 10px;
    background-color: rgba(0,0,0,0.35);
}
#page_startScreen QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    padding: 2px 6px 2px 6px;
    background-color: rgba(0,0,0,0.6);
    color: #ffffff;
}

/* Buttons */
#page_startScreen QPushButton#b_startGame {
    background-color: #3a3a3a;
    border: 2px solid #000000;
    border-radius: 0;
    padding: 6px 10px;
    color: #ffffff;
}
#page_startScreen QPushButton#b_startGame:hover {
    background-color: #4a4a4a;
}
#page_startScreen QPushButton#b_startGame:pressed {
    background-color: #2a2a2a;
    border-left-color: #111111;
    border-top-color: #111111;
    border-right-color: #555555;
    border-bottom-color: #555555;
}

/* Continue button states (keep green when enabled) */
#page_startScreen QPushButton#b_continue:disabled {
    background-color: #1f1f1f;
    border: 2px solid #444444;
    border-radius: 0;
    padding: 6px 10px;
    color: #888888;
}
#page_startScreen QPushButton#b_continue:enabled {
    background-color: #2e5f2e;
    border: 2px solid #163816;
    border-radius: 0;
    padding: 6px 10px;
    color: #c8ffc8;
}
#page_startScreen QPushButton#b_continue:enabled:hover {
    background-color: #357a3a;
}
#page_startScreen QPushButton#b_continue:enabled:pressed {
    background-color: #235f26;
    border-left-color: #133516;
    border-top-color: #133516;
    border-right-color: #5b8f60;
    border-bottom-color: #5b8f60;
}

/* ComboBox */
#page_startScreen QComboBox {
    background-color: #2f2f2f;
    border: 2px solid #000000;
    border-radius: 0;
    padding: 2px 6px;
    color: #ffffff;
}
#page_startScreen QComboBox::drop-down {
    width: 18px;
    border-left: 2px solid #000000;
    background-color: #3a3a3a;
}
#page_startScreen QComboBox::down-arrow {
    image: none;
}

/* KeySequenceEdit */
#page_startScreen QKeySequenceEdit {
    background-color: #2f2f2f;
    border: 2px solid #000000;
    border-radius: 0;
    padding: 2px 6px;
    color: #ffffff;
}

/* SpinBox (QAbstractSpinBox) */
#page_startScreen QSpinBox,
#page_startScreen QDoubleSpinBox,
#page_startScreen QAbstractSpinBox {
    background-color: #2f2f2f;
    border: 2px solid #000000;
    border-radius: 0;
    padding: 2px 6px;
    padding-right: 14px; /* room for up/down buttons */
    color: #ffffff;
    selection-background-color: #5a5a5a;
    selection-color: #ffffff;
}

#page_startScreen QAbstractSpinBox QLineEdit {
    background: transparent;
    border: none;
    padding: 0;
    margin: 0;
    color: #ffffff;
    font-family: Fixedsys, &quot;Lucida Console&quot;, &quot;Courier New&quot;, monospace;
    font-size: 10pt;
    qproperty-alignment: AlignVCenter | AlignRight;
}

#page_startScreen QAbstractSpinBox::up-button,
#page_startScreen QAbstractSpinBox::down-button {
    width: 12px;
    background-color: #3a3a3a;
    border-left: 2px solid #000000;
}

#page_startScreen QAbstractSpinBox::up-button {
    subcontrol-origin: border;
    subcontrol-position: top right;
    height: 50%;
    border-bottom: 1px solid #000000;
}

#page_startScreen QAbstractSpinBox::down-button {
    subcontrol-origin: border;
    subcontrol-position: bottom right;
    height: 50%;
    border-top: 1px solid #000000;
}

#page_startScreen QAbstractSpinBox::up-button:hover,
#page_startScreen QAbstractSpinBox::down-button:hover {
    background-color: #4a4a4a;
}

#page_startScreen QAbstractSpinBox::up-button:pressed,
#page_startScreen QAbstractSpinBox::down-button:pressed {
    background-color: #2a2a2a;
}

/* Labels */
#page_startScreen QLabel {
    background-color: transparent;
}

/* Specific small '?' button */
#page_startScreen QPushButton#b_aboutBoosts {
    background-color: transparent;
    border: 2px solid #ffffff;     /* white outline */
    border-radius: 12px;           /* circle for 25x25 */
    font-weight: bold;
    padding: 0;                    /* prevent size inflation */
    margin: 0;                     /* keep compact */
}
#page_startScreen QPushButton#b_aboutBoosts:hover {
    background-color: transparent;
    border: 2px solid #ffffff;   /* keep size consistent */
}

#page_startScreen QPushButton#b_aboutBoosts:pressed {
    background-color: transparent;
    border: 2px solid #ffffff;
}

/* FPS display labels in game view */
#page_gameView QLabel#l_fps,
#page_gameView QLabel#l_fpsNumber {
    font-family: Fixedsys, &quot;Lucida Console&quot;, &quot;Courier New&quot;, monospace; 
    font-size: 6pt; 
    font-weight: normal; 
    color: rgba(255, 255, 255, 0.75); 
    background-color: rgba(0, 0, 0, 0.25); 
    border: 1px solid rgba(0, 0, 0, 0.45); 
    border-radius: 0;
    padding: 0px 4px;
}

#page_gameView QLabel#l_fps {
    qproperty-alignment: AlignVCenter | AlignLeft;
}

#page_gameView QLabel#l_fpsNumber {
    qproperty-alignment: AlignCenter;
}

